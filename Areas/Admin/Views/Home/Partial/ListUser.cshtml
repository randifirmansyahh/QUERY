@model BlogDashboard

<b>Data User</b>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(item => item.user[0].Username)
            </th>
            <th>
                @Html.DisplayNameFor(item => item.user[0].Password)
            </th>
            <th>
                @Html.DisplayNameFor(item => item.user[0].Name)
            </th>
            <th>
                @Html.DisplayNameFor(item => item.user[0].Roles.Id)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.user)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Username)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Password)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Roles.Id)
                </td>
            </tr>
        }
    </tbody>
</table>